test D1 [[
    struct Person{
		int age = 20
		str name = "Ignacio"
	}
]]

test D2 [[
    struct Person{
		int age = 20
		str name = 20
	}
]] expect { "Expression should be str, found int" }

test D3 [[
    struct Address(str c){
		int housenumber = 221
		str city = "put here reference to variable c" 
	}
	struct Person{
		int age = 20
		str name = "Ignacio"
		Address address("Amsterdam")
	}
]]

test D4 [[
    struct Address(str c){
		int housenumber = 221
		str city = "put here reference to variable c" 
	}
	struct Person{
		int age = 20
		str name = "Ignacio"
		Address address("Amsterdam")[1]
	}
]] expect { "Setting size on a non-list element" }

test D5 [[
    struct Address(str c){
		int housenumber = 221
		str city = "put here reference to variable c" 
	}
	struct Person{
		int age = 20
		str name = "Ignacio"
		Address[] address("Amsterdam")
	}
]]

test D6 [[
    struct Address(str c){
		int housenumber = 221
		str city = c
	}
]] 

test D7 [[
    struct Address(bool c){
		int housenumber = 221
		str city = c
	}
]] expect { "Expression should be str, found bool" }

test D8 [[
    struct Address{
		u8 housenumber ?(> 25)
	}
]]

test D9 [[
    struct Address{
		Address housenumber ?(> 25)
	}
]]  

test D10 [[
    struct Address{
    	u8 housenumber ?(2 > 25)
	}
]]

test D11 [[
    struct Address{
    	u8 housenumber ?("2" > "25")
	}
]] expect { "Wrong operands for a comparator" }


test D12 [[
    struct Address{
    	bool housenumber ?("2" == 2)
	}
]] expect { "Wrong operands for equality" }

test D13 [[
    struct Address{
    	u8 housenumber ?("2" == "1")
	}
]]

test D14 [[
    struct Address{
    	u8 housenumber ?("2" == "1")
	}
]]

test D15 [[
    struct Address{
    	int foo = 5
    	u8 housenumber ?(foo > 1)
	}
]]

test D16 [[
    struct Address{
    	Address foo
    	u8 housenumber ?(foo == 4)
	}
]] expect { "Wrong operands for equality" }

test D17 [[
    struct Address{
    	u8 housenumber ?(this > 1)
	}
]]

test D18 [[
    struct Person{
		int age = 20
		str name = "Ignacio"
		Address address("Amsterdam")
	}
	struct Address(str c){
		int housenumber = 221
		str city = "put here reference to variable c" 
	}
]]

test D19 [[
    struct Foo{
		u8[] foo[3]
	}	
]]

test D20 [[
    struct Foo{
		u8 foo(3)
	}	
]] expect { "Constructor arguments only apply to user-defined types" }