test D1 [[
    struct Person{
    	int offset = 5
		u8[] bootCode[0x1fd - offset]
	}
]]

test D2 [[
    struct Person{
    	Address address
		u8[] bootCode[0x1fd - address.number]
	}
	
	struct Address{
		int number = 5
	}
]]

test D3 [[
    struct Person{
    	u8[] bootCode[0x1fd - bootCode.offset]
	}
]]

test D4 [[
    struct Person{
    	int foo = 5
    	u8[] bootCode[0x1fd - foo.offset]
	}
]] expect { "Only token types have offsets" }