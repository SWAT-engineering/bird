test D1 [[
    struct Person{
		int age = 20
		str name = "Ignacio"
	}
]]

test D2 [[
    struct Person{
		int age = 20
		str name = 20
	}
]] expect { "Expression should be str, found int" }

test D3 [[
    struct Address(str c){
		int housenumber = 221
		str city = "put here reference to variable c" 
	}
	struct Person{
		int age = 20
		str name = "Ignacio"
		Address address("Amsterdam")[1]
	}
]] expect { "Setting size on a non-list element" }

